{
  "name": "react-firebase-material-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-firebaseui": "^3.1.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "recompose": "^0.30.0"
  },
  "scripts": {
    "start": "react-scripts start;",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=development yarn build",
    "build:prod": "REACT_APP_ENV=production yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "shipit": "yarn build; yarn deploy",
    "shipit:dev": "yarn build:dev; yarn deploy",
    "shipit:prod": "yarn build:prod; yarn deploy",
    "deploy": "firebase deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-react-app": "^3.0.5",
    "standard": "^12.0.1"
  }
}
